   "source": [
    "from PIL import Image\n",
    "\n",
    "import os\n",
    "\n",
    "import glob\n",
    "\n",
    "path = os.path.join('test', '*.png')  ##拼接路径\n",
    " \n",
    "files = glob.glob(path)\n",
    "\n",
    "def ReshapeFiles():  \n",
    "    for item in files:  \n",
    "        im = Image.open(item)\n",
    "        out = im.resize((224,224))\n",
    "        out.save(item)\n",
    "\n",
    "ReshapeFiles()  "
   ]
